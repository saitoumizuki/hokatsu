<header class="header-nn">

<!-- <div class="test1"> -->
    <div class="n-box">
      <div class="logo-box">
        <%= link_to root_path do %>
        <p class="header-logo">Mochi mochi</p>
        <% end %>
      </div>
    </div>
<!-- </div> -->
<!-- <div class="test2"> -->
  <div class="n-box n-nav">
    <% if  user_signed_in? %>
      <nav>
        <ul class="list">
          <li class="item">
            <!-- スポット登録 -->
            <%= link_to " ", spots_path, class: "item-a a1 fas fa-map-marker-alt" %>
          </li>
          <li class="item">
            <!-- マイリスト -->
            <%= link_to " ",list_items_path(current_user), class: "item-a a2 fas fa-heart" %>
          </li>
          <li class="item mypage">
            <!-- マイページ -->
            <%= link_to " ", user_path(current_user.id), class: "item-a a3 fas fa-user" %>
          </li>
          <li class="item">
            <%= link_to "", users_sign_out_path, method: :delete, class: "item-a a9 fas fa-sign-out-alt" %>
          </li>
        </ul>
      </nav>

    <% elsif  admin_signed_in? %>

      <nav class="header-right">
        <ul class="list">
          <li class="item">
            <!-- 保育園の追加 -->
            <%= link_to "",new_nurseries_path, class: "item-a a7 fas fa-home" %>
          </li>
          <li class="item">
            <!-- ユーザー 一覧 -->
            <%= link_to "", users_path, class: "item-a a8 fas fa-users" %>
          </li>
          <li class="item">
            <!-- ログアウト -->
            <%= link_to "", destroy_admin_session_path, method: :delete, class: "item-a a9 fas fa-sign-out-alt" %>
          </li>
        </ul>
      </nav>

    <% else %>

      <nav class="header-right">
        <ul class="list">
          <li class="item">
            <!-- ログイン -->
            <%= link_to " ", new_user_session_path, class: "item-a a4 fas fa-sign-in-alt" %>
          </li>
          <li class="item">
            <!-- 新規登録 -->
            <%= link_to " ", new_user_registration_path, class: "item-a a5 fas fa-user-plus" %>
          </li>
          <li class="item">
            <!-- about -->
            <%= link_to " ","about_path", class: "item-a a6 fas fa-book" %>
          </li>
        </ul>
      </nav>

    <% end %>
    </div>

</header>


